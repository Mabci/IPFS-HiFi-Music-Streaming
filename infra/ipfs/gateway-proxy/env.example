# Configuración del Gateway Proxy
NODE_ENV=production
PORT=3001

# URLs del nodo IPFS local
IPFS_API_URL=http://ipfs-node:5001
IPFS_GATEWAY_URL=http://ipfs-node:8080

# Redis para cache
REDIS_URL=redis://redis:6379

# Base de datos para métricas (opcional)
DATABASE_URL=postgresql://postgres:password@postgres:5432/music_gateway

# Configuración de cache
CACHE_TTL=3600
MAX_CACHE_SIZE=1GB

# Logging
LOG_LEVEL=info
