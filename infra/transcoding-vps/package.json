  "main": "dist/workers/transcoding-worker-simple.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/workers/transcoding-worker-simple.js",
    "dev": "tsx src/workers/transcoding-worker-simple.ts",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop transcoding-worker",
    "pm2:restart": "pm2 restart transcoding-worker",
